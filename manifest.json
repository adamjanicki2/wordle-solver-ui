{
  "name": "Wordle Solver",
  "description": "A custom-built solver for Wordle that can solve a puzzle in an average of under 4 guesses!",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "build/index.html",
    "default_icon": {
      "16": "build/16.png",
      "32": "build/32.png",
      "48": "build/48.png",
      "128": "build/128.png"
    }
  },
  "icons": {
    "16": "build/16.png",
    "32": "build/32.png",
    "48": "build/48.png",
    "128": "build/128.png"
  }
}
